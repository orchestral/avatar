{
	"name": "orchestra/avatar",
	"description": "Avatar Package for Laravel and PHP",
	"license": "MIT",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "avatar", "gravatar"],
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Avatar\\": "src/Avatar/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "4.2.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"illuminate/auth": "4.2.*",
		"illuminate/config": "4.2.*",
		"illuminate/container": "4.2.*",
		"illuminate/database": "4.2.*"
	},
	"suggest": {
	    "laravel/framework": "Allow using orchestra/avatar component outside of Orchestra Platform 2",
	    "orchestra/foundation": "Allow using orchestra/avatar with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.3.x-dev"
		}
	},
	"minimum-stability": "dev"
}
